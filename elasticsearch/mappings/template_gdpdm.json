{
  "template": "gdpdm*",
  "settings": {
    "analysis": {
      "filter": {
        "ngrams": {
          "side": "front",
          "min_gram": 2,
          "max_gram": 255,
          "type": "edgeNGram"
        }
      },
      "analyzer": {
        "my_ngram": {
          "filter": [
            "standard",
            "lowercase",
            "ngrams"
          ],
          "type": "custom",
          "tokenizer": "standard"
        }
      }
    }
  },
  "mappings": {
    "_default_": {
      "dynamic": "strict"
    },
    "gwasviewer": {
      "properties": {
        "name": {
          "type": "string",
          "fields": {
            "partial": {
              "type": "string",
              "analyzer": "my_ngram",
              "search_analyzer": "standard"
            }
          }
        },
        "type": {
          "type": "string"
        },
        "comments": {
          "type": "string"
        },
        "maxscore": {
          "type": "double"
        },
        "numberofsnps": {
          "type": "long"
        },
        "published": {

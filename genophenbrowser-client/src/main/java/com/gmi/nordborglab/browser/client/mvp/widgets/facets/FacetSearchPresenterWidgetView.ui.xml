<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
        >
    <ui:style>
        .searchBox {
            display: inline-block;
            margin-top: 20px;
            margin-right: 20px;
        }

        .navPills {
            margin-top: 20px;
            margin-left: 0.5em;
        }
    </ui:style>

    <g:HTMLPanel>
        <div style="float:right;">
            <b:InputAddOn appendIcon="SEARCH" addStyleNames="{style.searchBox}" ui:field="searchBoxContainer">
                <b:TextBox ui:field="searchBox" placeholder="Search..."></b:TextBox>
            </b:InputAddOn>
        </div>
        <div style="float:left;">
            <b:NavPills addStyleNames="{style.navPills}" ui:field="navPills">
            </b:NavPills>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
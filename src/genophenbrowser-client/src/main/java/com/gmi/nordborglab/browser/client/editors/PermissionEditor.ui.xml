             xmlns:c="urn:import:com.google.gwt.user.cellview.client"
             xmlns:b='urn:import:org.gwtbootstrap3.client.ui'
        >

    <ui:style>

    </ui:style>
    <g:HTMLPanel>
        <div class="container-fluid">
            <div class="row">
                <div class="col-xs-12" style="heigh;230px;">
